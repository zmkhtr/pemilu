<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="10dp">

    <TextView
        android:id="@+id/textLayoutNama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="nama relawan"/>
    <TextView
        android:id="@+id/textLayoutAlamat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="alamat relawan"/>
    <TextView
        android:id="@+id/textLayoutTanggal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="tanggal lahir relawan"/>
    <TextView
        android:id="@+id/textLayoutJenis"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="jenis kelamin relawan"/>
    <TextView
        android:id="@+id/textLayoutPartai"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="partai relawan"/>

</LinearLayout>